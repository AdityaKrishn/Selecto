<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_store__fetch"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.aditya.selecto.Store_fetch">

    <EditText
        android:hint="KEY"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:id="@+id/key"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="45dp" />

    <Button
        android:text="FETCH"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/value"
        android:layout_alignLeft="@+id/key"
        android:layout_alignStart="@+id/key"
        android:layout_marginTop="28dp"
        android:id="@+id/fetch"
        android:onClick="fetch"
        android:background="@android:color/black" />

    <EditText
        android:hint="DATA"
        android:textAlignment="center"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp"
        android:id="@+id/value"
        android:layout_alignBottom="@+id/key"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="STORE"
        android:textColor="@color/white"
        android:background="@android:color/holo_green_light"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/store"
        android:onClick="store"
        android:layout_alignBaseline="@+id/fetch"
        android:layout_alignBottom="@+id/fetch"
        android:layout_alignRight="@+id/value"
        android:layout_alignEnd="@+id/value"
        android:layout_marginRight="26dp"
        android:layout_marginEnd="26dp" />
</RelativeLayout>
